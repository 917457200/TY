@model  BLL.Cookie.TeUser
@{
    ViewBag.Title = "index";
    Layout = "~/Views/Shared/MP.cshtml";
}
@section Head{
    <script src="~/Scripts/Menu.js" type="text/javascript"></script>

}
<body class="body">
    <div class="mainwrapper">
        @* 头部个人信息*@
        <div class="header">
            <div class="top">
                <div class="logo">
                    <a href="/home/index">
                        <img src="~/Content/xinjiemian/Index_logo.png" />
                    </a>
                </div>
                <div class="headerinner">
                    <div style="overflow: hidden;">
                        <div style="float: left; height: 80px; padding: 32px 0px 0px 10px;">
                            <img src="~/Content/xinjiemian/ProName1.png" />
                        </div>
                        <ul class="headmenu">
                            <li class="right">
                                <div class="userloggedinfo">

                                    <div class="userinfo">
                                        <h5>
                                            @Model.userName
                                            @switch( Model.userType )
                                                {
                                                    case "1":
                                                <small>-教师</small>
                                                        break;
                                                    case "2":
                                                <small>-学生</small>
                                                        break;
                                                    case "3":
                                                <small>-家长</small>
                                                        break;
                                                    default:
                                                        break;
                                                }
                                        </h5>
                                        <ul>
                                            <li>
                                                <a href="/Home/Index">返回列表</a>
                                            </li>
                                            <li>
                                                <a href="/Home/OutLogin">退出登录</a>
                                            </li>
                                        </ul>
                                    </div>

                                </div>
                            </li>
                        </ul>

                    </div>

                </div>
            </div>

        </div>
        <div class="conent">
            <div style="position: relative;">

                @* 菜单部分*@
                <div class="leftpanel">
                    <div class="leftmenu">
                        <ul class="nav nav-tabs nav-stacked">
                            <li class="nav-header">主菜单</li>
                        </ul>
                    </div>
                </div>
                @* 小导航菜单*@
                <div class="rightpanel">
                    <ul class="breadcrumbs" id="breadcrumbs">
                        <li class="right">
                            <a href="#" data-toggle="dropdown" class="dropdown-toggle">
                                <i class="icon-tint"></i> 颜色
                            </a>
                            <ul class="dropdown-menu pull-right skin-color">
                                <li><a class="default">蓝色</a></li>
                                <li><a class="red">红色</a></li>
                                <li><a class="green">绿色</a></li>
                                <li><a class="brown">灰色</a></li>
                            </ul>
                        </li>
                    </ul>
                    @* iframe 操作界面*@
                    <iframe width="100%" height="800px" frameborder="0" src="/main.html" scrolling="auto" onload="lowheight(this)" id="left" name="left"></iframe>
                </div>
            </div>
        </div>
        <div class="footer">

            <span> 东方博冠（北京）科技有限公司 版权所有 ©2016-2020</span>
            &nbsp; &nbsp; &nbsp; &nbsp;
            <span>技术支持: <a href="#">东方博冠（北京）科技有限公司 </a></span>

        </div>
    </div>
</body>
